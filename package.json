{
  "name": "hercule",
  "version": "2.0.0-alpha.1",
  "description": "Markdown transcluder",
  "main": "./lib/hercule",
  "scripts": {
    "test": "npm run compile && npm run unit-test && npm run integration-test",
    "posttest": "npm run lint",
    "unit-test": "./node_modules/.bin/nyc ./node_modules/.bin/ava",
    "integration-test": "./test/modules/bin/bats test/bats",
    "coverage": "./node_modules/.bin/nyc report --reporter=html && open coverage/index.html",
    "codecov": "./node_modules/.bin/nyc report --reporter=json && cat coverage/coverage-final.json | ./node_modules/.bin/codecov",
    "compile": "BABEL_ENV=compile ./node_modules/.bin/babel src --out-dir lib && ./node_modules/.bin/pegjs src/transclude.pegjs lib/transclude-parser.js",
    "install-bats": "./scripts/install-bats",
    "lint": "./node_modules/.bin/eslint ./src ./test"
  },
  "config": {
    "nyc": {
      "exclude": [
        "lib/transclude-parser.js",
        "node_modules/",
        "test/"
      ]
    }
  },
  "bin": {
    "hercule": "./bin/hercule"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesramsay/hercule.git"
  },
  "keywords": [
    "markdown",
    "transclude",
    "transclusion",
    "commonmark"
  ],
  "author": {
    "name": "James Ramsay",
    "url": "http://jramsay.com.au"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jamesramsay/hercule/issues"
  },
  "homepage": "https://github.com/jamesramsay/hercule",
  "dependencies": {
    "async": "^1.5.0",
    "dashdash": "^1.10.1",
    "duplexer2": "^0.1.4",
    "lodash": "^3.10.1",
    "request": "^2.65.0",
    "through2": "^2.0.0"
  },
  "devDependencies": {
    "assert-diff": "^1.0.1",
    "ava": "^0.7.0",
    "babel-cli": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "codecov.io": "^0.1.6",
    "eslint": "^1.10.1",
    "eslint-config-airbnb": "^1.0.0",
    "event-stream": "^3.3.2",
    "nock": "^3.0.0",
    "nyc": "^4.0.1",
    "pegjs": "^0.9.0"
  }
}
